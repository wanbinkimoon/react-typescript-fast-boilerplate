{
  "name": "archivio-enrico-appetito",
  "version": "1.8.0",
  "description": "editorial seciton of the appetito photo archive",
  "main": "main.tsx",
  "author": {
    "name": "Nicola Bertelloni",
    "email": "nicola.bertelloni@gmail.com",
    "url": "https://github.com/wanbinkimoon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fifthbeat/react-typescript-boilerplate"
  },
  "scripts": {
    "start": "webpack-dev-server --config internals/webpack/webpack.dev.js --mode development --hot --progress --color --port 5000",
    "build": "webpack --config internals/webpack/webpack.prod.js  --progress --colors",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "tslint -c tslint.json --fix './src/app/**.tsx' -t verbose"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit-no": "yarn lint && yarn version --patch --no-commit-hooks --no-git-tag-version && git add .",
      "pre-push-no": "yarn build && yarn version --minor --no-commit-hooks --no-git-tag-version && git add -A . && git commit --no-verify -m 'version number update'"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@types/classnames": "2.2.6",
    "@types/history": "4.7.2",
    "@types/node": "11.10.4",
    "@types/react": "^16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "4.0.32",
    "@types/react-router-redux": "5.0.16",
    "@types/redux-actions": "2.3.1",
    "@types/webpack": "4.4.16",
    "@types/webpack-env": "1.13.8",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "husky": "^3.1.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.4.4",
    "plop": "^2.1.0",
    "postcss": "^8.2.10",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.1.1",
    "postcss-reporter": "^6.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.11",
    "redux-devtools-extension": "^2.13.5",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-saga": "^0.10.5",
    "@types/reselect": "^2.2.0",
    "@types/styled-components": "^4.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "contentful": "^7.14.0",
    "dotenv-webpack": "^1.7.0",
    "normalize.css": "^8.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.0.2"
  }
}
